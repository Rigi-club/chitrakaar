{
  "compilerOptions": {
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "lib": ["es2017"],
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "LF",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist",
    "sourceMap": false,
    "strict": true,
    "target": "es2017"
  },
  "exclude": ["dist", "node_modules"]
}
